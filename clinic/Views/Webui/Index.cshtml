                    <div class="services-ion">
                        <span><i class="bi bi-award"></i></span>
                    </div>
                    <div class="services-cap">
                        <h5><a href="@Url.Action("educationservice","Webui")">Practical Conducted</a></h5>
                        <p>Introducing Practical Sessions at [EliteCare] Seminar Series.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="text-center">
        <a href="@Url.Action("educationservice","Webui")" class="btn">View Details</a>
    </div>
</div>
