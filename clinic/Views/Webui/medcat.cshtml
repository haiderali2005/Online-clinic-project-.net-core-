@model List<medicinecategory>;
@foreach(var item in Model)
{
    <div class="col-xl-4 col-lg-4 col-md-6">
        <div class="single-blogs mb-30">
            <div class="blog-img">
                <img src="@("~/catmedimages/"+item.medicinecat_image)" asp-append-version=true alt="">
            </div>
            <div class="blogs-cap">
                <h4>@item.medicinecat_name</h4>
                <a href="@Url.Action("medproducts","webui")" class="read-more1 mr-110">View medicines</a>
            </div>
        </div>
    </div>
}