@model List<clinicgallery>;
@foreach (var item in Model)
{
    <div class="col-xl-4 col-lg-4 col-md-6">
        <div class="single-blogs mb-30">
            <div class="blog-img">
                <img src="@("~/gallery/"+item.gallery_img)" asp-append-version=true alt="">
            </div>
        </div>
    </div>
}